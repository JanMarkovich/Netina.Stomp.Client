<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>AmirHosseinKhademi</Authors>
    <Company>ViraNasirFanavar</Company>
    <PackageProjectUrl>https://github.com/Netina/iNet.Stomp.Client</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Netina/iNet.Stomp.Client</RepositoryUrl>
    <PackageTags>stomp</PackageTags>
    <Version>1.0.21</Version>
    <SignAssembly>false</SignAssembly>
    <PackageId>iNet.Stomp.Client</PackageId>
    <Product>iNet.Stomp.Client</Product>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="Websocket.Client" Version="4.3.32" />
  </ItemGroup>
</Project>
